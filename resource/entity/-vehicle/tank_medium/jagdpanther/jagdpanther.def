{game_entity
	(include "/properties/tank.ext")
	(include "/properties/selection/vehicle.inc"  scale(1.2))
	{collider "tank.heavy"}
	{props "heavy" "-turret" "vision_vehicle_high" "muzzle_large"}

	{extender "enumerator"
		{digit_folder "ger"}
	}

	{volume "body1"
		{tags "body"}
		{component "body"}
		{thickness 50}
	}
	{volume "body2"
		{tags "body"}
		{component "body"}
		{thickness 50}
	}
	{volume "engine"
		{tags "engine"}
		{component "engine"}
		{thickness 50}
	}

	("armor_spa_heavy" armor(80 50 40 25))
	("armor_mantlet" t(100))
	("shield" args 10)
	
	{Volume "mantlet"
		{tags "gun"}
		{component "gun"}
		{thickness 100}
	}

	{extender "shield"
		{on}
	}
	
	{component "shieldl1" {tags "shield"}}
	{component "shieldl2" {tags "shield"}}
	{component "shieldl3" {tags "shield"}}
	{component "shieldl4" {tags "shield"}}
	{component "shieldl5" {tags "shield"}}
	{component "shieldr1" {tags "shield"}}
	{component "shieldr2" {tags "shield"}}
	{component "shieldr3" {tags "shield"}}
	{component "shieldr4" {tags "shield"}}
	{component "shieldr5" {tags "shield"}}
	
	("crew_5_human")

	{boarder
		{anm "driver"
			{forward	{begin	"board_jtiger_driver"} {end "seat_gunner_stand"} {base "open_driver"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
		{anm "left"
			{forward	{begin	"board_tank_right"} {end "seat_gunner_stand"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_left"} {end "seat_gunner_stand"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
		{anm "armor"
			{forward	{begin	"board_HighArmor"} 	{end "seat_armor"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "armor1"
			{forward	{begin	"board_HighArmor"} {end "seat_armor"}}
			{reverse 	{end	"emit_body"}}
		}
	}

    {sensor
        {visor "main"
            {vision "tank_main-h"}
			{bone "gun"}
        }
    }

	("seat_human" place(seat1) bone(seat01) door(emit5)  animation(armor1))
	("seat_human" place(seat2) bone(seat02) door(emit6)  animation(armor))
	("seat_human" place(seat3) bone(seat03) door(emit3)  animation(armor))
	("seat_human" place(seat4) bone(seat04) door(emit4)  animation(armor1))
	
	{extender "inventory"
		{box
			{item "bullet88 ap" 28}
			{item "bullet88 fg" 28}
			{item "ammo mgun" 600}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "88mm_pak43" filling "ammo bullet88 ap" 1}
			("abm_high")
		}
		{place "mgun"
			{type "mgun_hull"}
			{weapon "mg34" filled}
			{foresight "foresight3"}
			{gunner "driver2"}
			{charger "driver2"}
			("abm_mgun")
			{basic}
		}
	}

	{mass 45500}
	{Chassis
		("tank_trace" fx("tracks_big") step(0.47) len(1.18))
		{Locomotion ("locomotion")
			{maxspeed    46}
			{maxSpeedAtMaxTurn 13}
			{StartTime   15}
			{TurnTime    11}
			{TurnStart  1.9}			
			{TurnRadius  10} 
    		{TurnStop     1}
			{BrakeTime  1.5}
			{StopTime     3}
			{Gears   0.2 0.5 1}
			{RearGears 0.29}
		}
		{FuelBag
			{volume 720}	
			{rate	2.80}   
			{remain 720}
			{fuel "fuel"}
		}
		{speed
        	{Normal  10}
        	{Slow  16}
    	}
	}

	{Extension "jagdpanther.mdl"}
	
	{bone "gun_rot"
		{limits 10}
		{speed 0.003}
	}
	{bone "mgun"
		{limits 15}
		{speed2 80}
	}
}
