{game_entity
	(include "/properties/tank.ext")
	(include "/properties/selection/vehicle.inc"  scale(1.2))
	{props "medium" "turret_rotator" "vision_vehicle_high" "muzzle_medium"}

	{extender "enumerator"}
	{extender "detail"
		{random 0}
	}
	
	{Volume "body1"
		{component "body"}
		{tags "body"}
	}
	{Volume "body2"
		{component "body"}
		{tags "body"}
	}
	{Volume "body3"
		{component "body"}
		{tags "body"}
	}
	{Volume "turret1"
		{tags "turret"}
		{component "turret"}
	}
	{Volume "turret2"
		{tags "turret"}
		{component "turret"}
	}
	{Volume "mantlet"
		{tags "gun"}
		{component "gun"}
	}

	("armor_heavy" turret(150 150 150 25) body(100 76 38 19) body2(140 38 38 19))
	("armor_engine"  t(30 76 38 19))		
	("armor_mantlet" t(178))
	("shield" args 10)

	("shield" args 8)
	{extender "shield"
		{on}
	}

	{component "shieldl1" {tags "shield"}}
	{component "shieldl2" {tags "shield"}}
	{component "shieldr1" {tags "shield"}}
	{component "shieldr2" {tags "shield"}}

	("crew_4_human")

	("gunner_visible")
	("seat_human" place(seat1) bone(seat01) door(emit3)  animation(armor1))
	("seat_human" place(seat2) bone(seat02) door(emit4)  animation(armor))
	("seat_human" place(seat3) bone(seat03) door(emit5)  animation(armor))
	("seat_human" place(seat4) bone(seat04) door(emit6)  animation(armor1))

	{boarder
		{anm "left"
			{forward	{begin	"board_Kv"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_right"} {end "pose_mgun_pz6"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}

		{anm "armor"
			{forward	{begin	"board_HighArmor"} 	{end "seat_armor"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "armor1"
			{forward	{begin	"board_HighArmor"} {end "seat_armor"}}
			{reverse 	{end	"emit_body"}}
		}
	}


	{extender "inventory"
		{box
			{item "ammo bullet76 ap" 35}
			{item "ammo bullet76 fg" 35}
			;{item "ammo bullet76 apcr" 5}
			{item "browning" filled {user "mgun"}}
			{item "ammo hmgun" 300}
			{item "ammo mgun" 4750}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "76mm_m1a2" filling "ammo bullet76 ap" 1}	
			("abm_high")
		}
		{place "mgun"
			{type "mgun_aa"}
			{gunner "gunner2"}
			{charger "gunner2"}
			{LinkBone "mgun_rot"}
			{LinkAnimation "off"}
			("abm_mgun_heavy")
		}

		{place "mgun1"
			{type "mgun_hull"}
			{weapon "browning_m19" filled}
			{foresight "foresight4"}
			{gunner "driver"}
			{charger "driver"}
		}
		{place "mgun2"
			{type "mgun_coaxial"}
			{weapon "browning_m19" filled}
			{foresight "foresight5"}
			{basic}
		}
	}

{Extension "m4a3e2_jumbo_76.mdl"}

	{mass 42900}
	{Chassis
		("tank_trace" fx("tracks_sm") step(0.3) len(0.55))
		{Locomotion ("locomotion")
			{maxspeed   27}
			{maxSpeedAtMaxTurn 11}
			{StartTime  15}
			{TurnTime   11}
			{TurnStart  1.9}			
			{TurnRadius 10} 
    		{TurnStop   0.8}
			{BrakeTime  1.5}
			{StopTime   2.5}
			{Gears   0.2 0.4 1}
			{RearGears  0.34}
		}
		{FuelBag
			{volume 640 }	
			{rate	220 }   
			{remain 640 }
			{fuel "fuel"}
		}
		{speed
        	{Normal  10}
        	{Slow  16}
    	}
	}
	
	{bone "turret"
		{speed 0.00888888888888888888888888888889}
	}
	
	{bone "turret_mgun"
		{limits -15 15}
		{speed 0.03}
		{terminator}
	}
}

